{
  "name": "@dept/ui",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "start-storybook --quiet --no-open -p 3002",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "generate": "yarn generate:theme-typings && yarn generate:barrels",
    "generate:theme-typings": "npx @chakra-ui/cli tokens ./src/theme/theme.ts",
    "generate:barrels": "barrelsby -c barrelsby.json --delete -q && npx prettier --write './**/index.{ts,tsx}'",
    "storybook": "start-storybook",
    "postinstall": "yarn generate:theme-typings",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.0",
    "@dept/config": "*",
    "@dept/icons": "*",
    "@dept/tsconfig": "*",
    "@tanem/react-nprogress": "^5.0.16",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.12",
    "polished": "^4.1.1",
    "react": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-select": "^5.4.0",
    "react-use": "^17.3.2",
    "rosetta": "^1.1.0",
    "swiper": "^8.1.4",
    "yup": "^0.32.11",
    "zustand": "^3.5.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.2.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/react": "^13.2.0",
    "@types/react": "^18.0.21",
    "@types/swiper": "^6.0.0",
    "barrelsby": "^2.5.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "storybook-addon-swc": "^1.1.7",
    "typescript": "4.9.3"
  }
}
