{
  "name": "@dept/ui",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "storybook dev --quiet --no-open -p 3002",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "generate": "yarn generate:theme-typings && yarn generate:barrels",
    "generate:theme-typings": "npx @chakra-ui/cli tokens ./src/theme/theme.ts",
    "generate:barrels": "barrelsby -c barrelsby.json --delete -q && npx prettier --write './**/index.{ts,tsx}'",
    "storybook": "storybook dev",
    "postinstall": "yarn generate:theme-typings",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@dept/config": "*",
    "@dept/icons": "*",
    "@dept/tsconfig": "*",
    "@emotion/styled": "^11.11.0",
    "@tanem/react-nprogress": "^5.0.51",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.14",
    "polished": "^4.3.1",
    "react": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-select": "^5.8.0",
    "react-use": "^17.5.0",
    "rosetta": "^1.1.0",
    "swiper": "^11.0.7",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-controls": "^8.0.0",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-storysource": "^8.0.0",
    "@storybook/addon-viewport": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.67",
    "@types/swiper": "^6.0.0",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "storybook": "^8.0.0",
    "typescript": "5.4.2"
  }
}
