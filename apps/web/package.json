{
  "name": "@dept/web",
  "version": "1.0.0",
  "description": "web application",
  "scripts": {
    "start": "NODE_ENV=production next start",
    "dev": "next dev",
    "predev": "yarn generate-theme-typings",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prebuild": "yarn generate-theme-typings",
    "export": "next build && next export",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './src/**/*'",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli@5 ./src/icons --template ./src/icons/svgrtemplate.js --ext tsx --out-dir ./src/icons/components --ref --index-template ./src/icons/index-template.js"
  },
  "author": "DEPTÂ®",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@dept/config": "workspace:^",
    "@dept/tsconfig": "workspace:^",
    "@dept/ui": "workspace:^",
    "@next/bundle-analyzer": "^14.2.0",
    "@testing-library/dom": "^10.0.0",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "dot-object": "^2.1.4",
    "dotenv": "^16.4.5",
    "downshift": "^9.0.4",
    "favicons": "^7.2.0",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.28",
    "immer": "^10.0.4",
    "intersection-observer": "^0.12.2",
    "lodash.throttle": "^4.1.1",
    "next": "~14.2.0",
    "next-seo": "^6.5.0",
    "next-sitemap": "^4.2.3",
    "polished": "^4.3.1",
    "react": "^18.2.0",
    "react-calendar": "^4.8.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-icons": "^5.0.1",
