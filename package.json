{
  "name": "dept-react-setup",
  "version": "2.1.0",
  "description": "React setup for Dept",
  "engines": {
    "npm": "please use yarn",
    "node": "16"
  },
  "scripts": {
    "start": "NODE_ENV=production next start",
    "dev": "next dev",
    "predev": "yarn generate-theme-typings",
    "build": "next build",
    "prebuild": "yarn generate-theme-typings",
    "export": "next build && next export",
    "postinstall": "husky install && yarn generate-theme-typings",
    "generate-theme-typings": "npx @chakra-ui/cli tokens ./src/theme/theme.ts",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './src/**/*'",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "route": "dept-react-scripts route",
    "context": "dept-react-scripts context",
    "component": "dept-react-scripts component",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli@5 ./src/icons --template ./src/icons/svgrtemplate.js --ext tsx --out-dir ./src/icons/components --ref"
  },
  "author": "Dept",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.(js|json|ts|tsx)": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/is-prop-valid": "^1.1.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@next/bundle-analyzer": "^11.0.0",
    "@sentry/browser": "^6.2.2",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "dot-object": "^2.1.4",
    "dotenv": "^10.0.0",
    "downshift": "^6.1.0",
    "express": "^4.17.1",
    "favicons": "^6.2.1",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "formik": "^2.2.6",
    "framer-motion": "^6.3.3",
    "husky": "5.1.3",
    "immer": "^9.0.3",
    "intersection-observer": "^0.12.0",
    "jest": "^28.0.2",
    "jest-axe": "^6.0.0",
    "lodash.throttle": "^4.1.1",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.5.2",
    "next-seo": "^5.4.0",
    "polished": "^4.1.1",
    "react": "^18.2.0",
    "react-calendar": "^3.3.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-icons": "^4.2.0",
    "react-ink": "^6.4.0",
    "react-intersection-observer": "^8.31.0",
    "react-number-format": "^4.5.0",
    "react-popper": "^2.2.4",
    "react-scroll": "^1.8.2",
    "react-use": "^17.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-to-element": "^2.0.3",
    "sharp": "^0.29.2",
    "yup": "^0.32.11",
    "zustand": "^3.5.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.1",
    "@chakra-ui/cli": "^2.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@swc/core": "^1.2.161",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/dot-object": "^2.1.2",
    "@types/express": "^4.17.12",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^15.12.2",
    "@types/react": "^18.0.14",
    "@types/react-calendar": "^3.4.0",
    "@types/react-dom": "^18.0.5",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-modal": "^3.12.0",
    "@types/react-scroll": "^1.8.2",
    "@types/scroll-to-element": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "circular-dependency-plugin": "^5.2.2",
    "dept-react-scripts": "^1.2.2",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest-environment-jsdom": "^28.0.2",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "storybook-addon-swc": "^1.1.7",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4"
  }
}
