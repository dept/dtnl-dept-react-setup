{
  "name": "dept-react-setup",
  "version": "2.0.2",
  "description": "React setup for Dept",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production next start",
    "dev": "next dev",
    "build": "next build",
    "start:custom-server": "NODE_ENV=production node .next/server/server",
    "dev:customer-server": "nodemon",
    "export": "next build && next export",
    "postbuild": "tsc --project server/tsconfig.json",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './{src,server}/**/*'",
    "lint": "eslint './{src,server}/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "route": "dept-react-scripts route",
    "context": "dept-react-scripts context",
    "component": "dept-react-scripts component",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli ./src/icons --template ./src/icons/svgrtemplate.js --ext tsx --out-dir ./src/icons/components"
  },
  "author": "Dept",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "contributors": [
    {
      "name": "Tim de Wolf",
      "email": "tim.dewolf@deptagency.com"
    },
    {
      "name": "Robby Uitbeijerse",
      "email": "robby.uitbeijerse@deptagency.com"
    }
  ],
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.(js|json|ts|tsx)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.2",
    "@popperjs/core": "^2.5.4",
    "@reach/dialog": "^0.12.0",
    "@reach/menu-button": "^0.12.0",
    "@sentry/browser": "^5.27.6",
    "@styled-system/css": "^5.1.5",
    "@welldone-software/why-did-you-render": "^5.0.0",
    "axios": "^0.21.0",
    "date-fns": "^2.16.1",
    "dot-object": "^2.1.4",
    "dotenv": "^8.2.0",
    "downshift": "^6.0.6",
    "express": "^4.17.1",
    "favicons": "^6.2.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "formik": "^2.2.5",
    "framer-motion": "^2.9.4",
    "husky": "^4.3.0",
    "immer": "^8.0.0",
    "intersection-observer": "^0.11.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^10.5.1",
    "lodash.throttle": "^4.1.1",
    "next": "^10.0.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^3.1.5",
    "next-seo": "^4.17.0",
    "polished": "^4.0.4",
    "react": "^17.0.1",
    "react-calendar": "^3.2.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-icons": "^3.11.0",
    "react-ink": "^6.4.0",
    "react-input-mask": "^2.0.4",
    "react-intersection-observer": "^8.30.3",
    "react-number-format": "^4.4.1",
    "react-popper": "^2.2.4",
    "react-scroll": "^1.8.1",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.32.4",
    "scroll-to-element": "^2.0.3",
    "styled-components": "5.2.1",
    "styled-system": "^5.1.5",
    "webpack": "^4",
    "yup": "0.29.3",
    "zustand": "^3.2.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.0.0",
    "@storybook/addon-docs": "^6.1.2",
    "@storybook/addon-knobs": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addon-storysource": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/express": "4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-calendar": "^3.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-modal": "^3.10.6",
    "@types/react-scroll": "^1.8.2",
    "@types/react-transition-group": "^4.4.0",
    "@types/scroll-to-element": "^2.0.0",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.14",
    "@types/yup": "0.29.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-styled-components": "^1.12.0",
    "circular-dependency-plugin": "^5.2.2",
    "dept-react-scripts": "^1.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
