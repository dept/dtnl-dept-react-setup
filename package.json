{
  "name": "dept-react-setup",
  "version": "2.0.0",
  "description": "React setup for Dept",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "next build",
    "export": "next export",
    "postbuild": "tsc --project server/tsconfig.json",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './{src,server}/**/*'",
    "lint": "eslint src/ server/ --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "start": "NODE_ICU_DATA=node_modules/full-icu NODE_ENV=production node .next/server",
    "generate": "hygen generator",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli ./public/icons --template ./public/icons/svgrtemplate.js --ext tsx --out-dir ./public/icons/components"
  },
  "author": "Dept",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "contributors": [
    {
      "name": "Tim de Wolf",
      "email": "tim.dewolf@deptagency.com"
    },
    {
      "name": "Robby Uitbeijerse",
      "email": "robby.uitbeijerse@deptagency.com"
    }
  ],
  "lint-staged": {
    "**/*.(js|json|ts|tsx)": [
      "eslint",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^9.3.2",
    "@sentry/browser": "^5.15.4",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.1",
    "dotenv": "^8.2.0",
    "downshift": "^5.0.3",
    "express": "^4.17.1",
    "favicons": "^6.0.0",
    "formik": "^2.1.4",
    "full-icu": "^1.3.1",
    "husky": "^4.2.3",
    "immer": "^6.0.2",
    "intersection-observer": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^25.2.3",
    "jest-axe": "^3.4.0",
    "lint-staged": "^10.0.10",
    "lodash.throttle": "^4.1.1",
    "next": "^9.3.2",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.0",
    "next-seo": "^4.4.0",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-axe": "^3.4.1",
    "react-calendar": "^2.19.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-ink": "^6.4.0",
    "react-intersection-observer": "^8.26.1",
    "react-modal": "^3.11.2",
    "react-popper": "^1.3.7",
    "react-portal": "^4.2.1",
    "react-quill": "^1.3.3",
    "react-scroll": "^1.7.16",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.3.0",
    "react-use": "^13.27.1",
    "regenerator-runtime": "^0.13.5",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5",
    "webpack": "^4",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/express": "^4.17.3",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest-axe": "^3.2.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.10.5",
    "@types/react-scroll": "^1.5.4",
    "@types/react-select": "^3.0.11",
    "@types/styled-components": "^5.0.1",
    "@types/styled-system": "^5.1.9",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "circular-dependency-plugin": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "hygen": "^5.0.3",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "opn": "^6.0.0",
    "prettier": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
