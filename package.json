{
  "name": "dept-react-setup",
  "version": "2.0.2",
  "description": "React setup for Dept",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ICU_DATA=node_modules/full-icu NODE_ENV=production node .next/server/server",
    "dev": "nodemon",
    "build": "next build",
    "export": "next build && next export",
    "postbuild": "tsc --project server/tsconfig.json",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './{src,server}/**/*'",
    "lint": "eslint './{src,server}/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "route": "dept-react-scripts route",
    "context": "dept-react-scripts context",
    "component": "dept-react-scripts component",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli ./public/icons --template ./public/icons/svgrtemplate.js --ext tsx --out-dir ./public/icons/components"
  },
  "author": "Dept",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "contributors": [
    {
      "name": "Tim de Wolf",
      "email": "tim.dewolf@deptagency.com"
    },
    {
      "name": "Robby Uitbeijerse",
      "email": "robby.uitbeijerse@deptagency.com"
    }
  ],
  "lint-staged": {
    "**/*.(js|json|ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^9.5.2",
    "@popperjs/core": "^2.4.4",
    "@sentry/browser": "^5.21.1",
    "@styled-system/css": "^5.1.5",
    "core-js": "^3.6.5",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "downshift": "^6.0.5",
    "express": "^4.17.1",
    "favicons": "^6.2.0",
    "formik": "^2.1.5",
    "framer-motion": "^2.5.5",
    "full-icu": "^1.3.1",
    "husky": "^4.2.5",
    "immer": "^7.0.7",
    "intersection-observer": "^0.11.0",
    "jest": "^26.4.1",
    "jest-axe": "^3.5.0",
    "lint-staged": "^10.2.11",
    "lodash.throttle": "^4.1.1",
    "next": "^9.5.2",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.2",
    "next-seo": "^4.7.3",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-axe": "^3.5.3",
    "react-calendar": "^3.1.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-ink": "^6.4.0",
    "react-intersection-observer": "^8.27.1",
    "react-modal": "^3.11.2",
    "react-popper": "^2.2.3",
    "react-portal": "^4.2.1",
    "react-scroll": "^1.8.1",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.3",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "webpack": "^4",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.0.16",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storysource": "^6.0.16",
    "@storybook/react": "^6.0.16",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.9",
    "@types/express": "4.17.1",
    "@types/helmet": "^0.0.48",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest-axe": "^3.5.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-calendar": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-scroll": "^1.8.0",
    "@types/react-select": "^3.0.19",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.13",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@welldone-software/why-did-you-render": "^4.3.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.11.1",
    "circular-dependency-plugin": "^5.2.0",
    "cz-conventional-changelog": "3.2.0",
    "dept-react-scripts": "^1.0.7",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "3.9.7"
  }
}
