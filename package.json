{
  "name": "dept-react-setup",
  "version": "2.0.2",
  "description": "React setup for Dept",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production next start",
    "dev": "next dev",
    "build": "next build",
    "start:custom-server": "NODE_ICU_DATA=node_modules/full-icu NODE_ENV=production node .next/server/server",
    "dev:customer-server": "nodemon",
    "export": "next build && next export",
    "postbuild": "tsc --project server/tsconfig.json",
    "type-check": "tsc --noEmit",
    "test": "jest --config test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node  --inspect-brk ./node_modules/bin/jest/jest.js --config test/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --write './{src,server}/**/*'",
    "lint": "eslint './{src,server}/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "yarn lint --fix",
    "validate": "npm-run-all --parallel type-check lint test -s build",
    "route": "dept-react-scripts route",
    "context": "dept-react-scripts context",
    "component": "dept-react-scripts component",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "favicons": "node config/favicons",
    "svgr": "npx @svgr/cli ./src/icons --template ./src/icons/svgrtemplate.js --ext tsx --out-dir ./src/icons/components"
  },
  "author": "Dept",
  "repository": "bitbucket:tamtam-nl/dtnl-dept-react-setup",
  "contributors": [
    {
      "name": "Tim de Wolf",
      "email": "tim.dewolf@deptagency.com"
    },
    {
      "name": "Robby Uitbeijerse",
      "email": "robby.uitbeijerse@deptagency.com"
    }
  ],
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.(js|json|ts|tsx)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^9.5.5",
    "@popperjs/core": "^2.5.3",
    "@sentry/browser": "^5.25.0",
    "@styled-system/css": "^5.1.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "downshift": "^6.0.6",
    "express": "^4.17.1",
    "favicons": "^6.2.0",
    "formik": "^2.2.0",
    "framer-motion": "^2.9.0",
    "full-icu": "^1.3.1",
    "husky": "^4.3.0",
    "immer": "^7.0.9",
    "intersection-observer": "^0.11.0",
    "jest": "^26.5.3",
    "jest-axe": "^4.0.0",
    "lint-staged": "^10.4.0",
    "lodash.throttle": "^4.1.1",
    "next": "^9.5.5",
    "next-compose-plugins": "^2.2.0",
    "next-pwa": "^3.1.5",
    "next-seo": "^4.11.0",
    "polished": "^4.0.2",
    "react": "^16.13.1",
    "react-calendar": "^3.1.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-ink": "^6.4.0",
    "react-intersection-observer": "^8.29.0",
    "react-modal": "^3.11.2",
    "react-popper": "^2.2.3",
    "react-scroll": "^1.8.1",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.0.0",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/express": "4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest-axe": "^3.5.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@types/react-calendar": "^3.1.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-scroll": "^1.8.0",
    "@types/react-select": "^3.0.22",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.14",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@welldone-software/why-did-you-render": "^4.3.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-styled-components": "^1.11.1",
    "circular-dependency-plugin": "^5.2.0",
    "dept-react-scripts": "^1.0.12",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
